{
  "compileOnSave": false,
  "compilerOptions": {
    "moduleResolution": "node",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "target": "es6",
    "module": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "downlevelIteration": true,
    "jsx": "react-native",
    "noEmit": true,
    "resolveJsonModule": true
  }
}
